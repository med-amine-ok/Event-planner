{
  "builds": [
    {
      "src": "EventPlanner/wsgi.py",
      "use": "@vercel/python"
    },
    { "src": "static/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "static/$1" },
    { "src": "/media/(.*)", "dest": "EventPlanner/wsgi.py" },
    { "src": "/(.*)", "dest": "EventPlanner/wsgi.py" }
  ]
}
